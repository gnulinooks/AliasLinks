{
  "name": "Alias Links",
  "manifest_version": 2,
  "version": "1.4.1",
  "description": "Aliasing the links that we hardly remember. Set a unique word for a link.",
  "omnibox": {"keyword": "a"},
  "icons" : {
	"16" : "barIcon.png"
	},
  "background":{
      "scripts": ["background.js"],
      "persistent": false
    },
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup":"popup.html"
  },
  "permissions": [
  "notifications",
	"tabs",
  "background",
  "storage"
  ]
}
